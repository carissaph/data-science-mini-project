    "select b.business_id, b.categories, b.postal_code, r.stars  \n",
    "from Business as b, Review as r\n",
    "where city=\"Philadelphia\" and (b.categories like'%restaurant%' and b.categories like '%food%' and  b.categories like '%Italian%'  and b.business_id = r.business_id and r.stars > 4)\n",
    "\n",
    "limit 100;"
