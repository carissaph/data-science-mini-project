    "%%sql \n",
    "select state, count(distinct(business_id)) as \"Number of Restaurants\"\n",
    "from Business as b\n",
    "where b.categories like '%food%' or b.categories like '%Restaurant%'\n",
    "group by state\n",
    "order by count(distinct(business_id)) desc\n",
    "limit 10;"
   ]
  },
