    "%%sql \n",
    "select b.business_id, b.categories, r.stars\n",
    "from Business as b, Review as r\n",
    "where city=\"Philadelphia\" and b.postal_code = '19129' and (categories like'%restaurant%' and b.categories like '%Nightlife%'  and b.business_id = r.business_id and r.stars > 4)\n",
    "limit 50;"
